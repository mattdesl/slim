<!--
    Document   : simple.xml
    Created on : November 22, 2007, 9:23 PM
    Author     : Matthias Mann
    Description:
        This is a very simple looking theme using a fake 3d look and shades of gray.
        It can be used as a base for new applications / projects.
--><themes>
    <images file="simple.png" format="COLOR">
        <area name="-borderA" border="3" xywh="0,0,9,9" splitx="L3,R3" splity="T3,B3"/>
        <area name="-borderB" border="3" xywh="10,0,9,9" splitx="L3,R3" splity="T3,B3"/>
        <area name="-borderC" border="3" xywh="20,0,9,9" splitx="L3,R3" splity="T3,B3"/>
        <area name="-borderD" border="3" xywh="30,0,9,9" splitx="L3,R3" splity="T3,B3"/>
        <area name="-borderE" border="3" xywh="40,0,9,9" splitx="L3,R3" splity="T3,B3"/>
        <area name="-borderF" border="3" xywh="50,0,9,9" splitx="L3,R3" splity="T3,B3"/>
        <area name="-borderG" border="3" xywh="60,0,9,9" splitx="L3,R3" splity="T3,B3"/>
        <area name="-borderH" border="3" xywh="70,0,9,9" splitx="L3,R3" splity="T3,B3"/>
        <area name="-iconA" xywh="0,10,9,6"/>
        <area name="-iconB" xywh="0,16,10,9"/>
        <area name="-iconC" xywh="10,10,14,14"/>
        <area name="-iconD" xywh="0,25,9,6"/>
        <area name="-iconE" xywh="0,31,6,9"/>
        <area name="-iconF" xywh="6,31,6,9"/>
        <area name="-iconG" xywh="28,12,7,7"/>
        <!-- iconH icon based on famfamfam Icon "arrow_turn_right.png" -->
        <area name="-iconH" xywh="68,10,15,15"/>
        <!-- iconI icon based on famfamfam Icon "folder.png" -->
        <area name="-iconI" xywh="53,10,14,11"/>
        <!-- iconJ icon based on famfamfam Icon "key.png" -->
        <area name="-iconJ" xywh="24,41,16,16"/>
        <!-- iconK icon based on famfamfam Icon "arrow_refresh.png" -->
        <area name="-iconK" xywh="41,41,16,16"/>
        <!-- iconL icon based on famfamfam Icon "book_open.png" -->
        <area name="-iconL" xywh="36,10,16,14"/>
        <area name="-iconM" xywh="20,41,3,13"/>
        <area name="-iconN" xywh="30,20,4,4"/>
        <area name="-iconO" xywh="29,25,12,12"/>
        <area name="-iconP" xywh="42,25,12,12"/>
        <area name="-iconQ" xywh="0,51,13,3"/>
        <area name="-iconR" xywh="58,41,13,16"/>
        <area name="-gradA" xywh="0,62,128,1"/>
        <area name="-gradB" xywh="0,59,128,1"/>
        <area name="-gradC" xywh="25,21,1,1"/>
        <area name="-selH" repeatX="true" tiled="true" xywh="26,10,8,1"/>
        <area name="-selV" repeatY="true" tiled="true" xywh="26,10,1,8"/>

        <area name="white" xywh="24,4,1,1"/>
        <alias name="black" ref="white" tint="#000"/>

        <alias name="frame.resizeIcon" ref="-iconC"/>

        <area name="textarea.ul-bullet" center="true" sizeOverwriteH="20" inset="3,0,-3,0" xywh="80,0,9,9"/>
        
        <composed name="frame.resizeable-title" border="25,5,5,5">
            <alias ref="-borderA"/>
            <grid weightsX="0,1,0" weightsY="0,1" inset="4">
                <alias ref="none"/>
                <select sizeOverwriteV="18">
                    <alias ref="-gradA" if="keyboardFocus | hasOpenPopups"/>
                    <alias ref="-gradB"/>
                </select>
                <alias ref="none"/>
                <alias ref="none"/>
                <alias ref="none"/>
                <alias ref="none"/>
            </grid>
        </composed>

        <composed name="--selection">
            <grid weightsX="1" weightsY="0,1,0">
                <alias ref="-selH"/>
                <alias ref="none"/>
                <alias ref="-selH"/>
            </grid>
            <grid weightsX="0,1,0" weightsY="1">
                <alias ref="-selV"/>
                <alias ref="none"/>
                <alias ref="-selV"/>
            </grid>
        </composed>
        <alias name="-selection" ref="--selection" inset="2" if="keyboardFocus"/>

        <select name="button.background" border="6,3">
            <alias ref="-borderB" if="disabled" tint="#8fff"/>
            <alias ref="-borderE" if="armed"/>
            <alias ref="-borderB"/>
        </select>
        <alias name="button.overlay" ref="-selection"/>

        <select name="togglebutton.background" border="6,3">
            <alias ref="-borderC" if="disabled + selected" tint="#8fff"/>
            <alias ref="-borderB" if="disabled" tint="#8fff"/>
            <alias ref="-borderE" if="armed"/>
            <alias ref="-borderC" if="selected"/>
            <alias ref="-borderB"/>
        </select>
        <alias name="togglebutton.overlay" ref="-selection"/>

        <grid name="radiobutton.background" weightsX="0,1" weightsY="1" border="1,14,1,1">
            <select center="true">
                <composed if="selected + disabled" tint="#8fff">
                    <alias ref="-iconP"/>
                    <alias ref="-iconN" center="true"/>
                </composed>
                <alias ref="-iconO" if="disabled" tint="#8fff"/>
                <composed if="selected + armed">
                    <alias ref="-iconP"/>
                    <alias ref="-iconN" center="true"/>
                </composed>
                <alias ref="-iconP" if="armed"/>
                <composed if="selected">
                    <alias ref="-iconO"/>
                    <alias ref="-iconN" center="true"/>
                </composed>
                <alias ref="-iconO"/>
            </select>
            <alias ref="none"/>
        </grid>
        <alias name="radiobutton.overlay" ref="--selection" if="keyboardFocus" inset="1,14,1,1"/>

        <select name="valueadjuster.display.background">
            <alias ref="-borderB" if="disabled" tint="#8fff"/>
            <alias ref="-borderE" if="armed | pressed"/>
            <alias ref="-borderB"/>
        </select>
        <alias name="valueadjuster.display.overlay" ref="-selection"/>

        <alias name="-frame.closeIcon" ref="-iconB" inset="2,3,3,3"/>
        <select name="frame.closebutton.background">
            <composed if="armed">
                <alias ref="-borderE"/>
                <alias ref="-frame.closeIcon" inset="1,1,-1,-1"/>
            </composed>
            <composed>
                <alias ref="-borderB"/>
                <alias ref="-frame.closeIcon"/>
            </composed>
        </select>


        <alias name="-icon.upbutton" ref="-iconD" inset="5,3,5,4"/>
        <alias name="-icon.downbutton" ref="-iconA" inset="5,3,5,4"/>
        <alias name="-icon.leftbutton" ref="-iconE" inset="3,4,4,6"/>
        <alias name="-icon.rightbutton" ref="-iconF" inset="3,4,4,6"/>

        <alias name="scrollbar.thumb.background" ref="valueadjuster.display.background"/>
        <area name="scrollbar.background" tiled="true" xywh="12,24,16,16"/>

        <select name="vscrollbar.upbutton.background">
            <composed if="armed">
                <alias ref="-borderD"/>
                <alias ref="-icon.upbutton" inset="1,1,-1,-1"/>
            </composed>
            <composed>
                <alias ref="-borderA"/>
                <alias ref="-icon.upbutton"/>
            </composed>
        </select>
        <select name="vscrollbar.downbutton.background">
            <composed if="armed">
                <alias ref="-borderD"/>
                <alias ref="-icon.downbutton" inset="1,1,-1,-1"/>
            </composed>
            <composed>
                <alias ref="-borderA"/>
                <alias ref="-icon.downbutton"/>
            </composed>
        </select>

        <select name="hscrollbar.leftbutton.background">
            <composed if="armed">
                <alias ref="-borderD"/>
                <alias ref="-icon.leftbutton" inset="1,1,-1,-1"/>
            </composed>
            <composed>
                <alias ref="-borderA"/>
                <alias ref="-icon.leftbutton"/>
            </composed>
        </select>
        <select name="hscrollbar.rightbutton.background">
            <composed if="armed">
                <alias ref="-borderD"/>
                <alias ref="-icon.rightbutton" inset="1,1,-1,-1"/>
            </composed>
            <composed>
                <alias ref="-borderA"/>
                <alias ref="-icon.rightbutton"/>
            </composed>
        </select>

        <composed name="splitpane.splitter.background" sizeOverwriteH="9" sizeOverwriteV="9" border="0">
            <alias ref="button.background"/>
            <alias ref="-iconM" center="true" inset="0,-1,0,1"/>
        </composed>
        <composed name="splitpane.vsplitter.background" sizeOverwriteH="9" sizeOverwriteV="9" border="0">
            <alias ref="button.background"/>
            <alias ref="-iconQ" center="true" inset="-1,0,1,0"/>
        </composed>

        <select name="editfield.background">
            <alias ref="-borderE" if="readonly"/>
            <alias ref="-borderC"/>
        </select>
        <area name="-editfield.cursor" xywh="24,10,1,1"/>
        <animation name="editfield.cursor" timeSource="keyboardFocus">
            <frame ref="-editfield.cursor" duration="500"/>
            <frame ref="none" duration="500"/>
        </animation>
        <alias name="editfield.selection" ref="-gradC"/>

        <alias name="listbox.display.background" ref="-gradC" if="selected"/>
        <alias name="listbox.display.overlay" ref="-selection"/>

        <alias name="-icon.combobox" ref="-iconA" center="true"/>
        <select name="combobox.button.background" inset="2,0,2,2">
            <composed if="armed">
                <alias ref="-borderD"/>
                <alias ref="-icon.combobox" inset="1,1,-1,-1"/>
            </composed>
            <composed>
                <alias ref="-borderA"/>
                <alias ref="-icon.combobox"/>
            </composed>
        </select>
        <alias name="combobox.display.background" ref="-gradC" inset="2,2,2,0" if="comboboxKeyboardFocus"/>

        <alias name="-icon.checkbox" ref="-iconG" center="true"/>
        <select name="checkbox.background">
            <composed if="selected + armed">
                    <alias ref="-borderE"/>
                    <alias ref="-icon.checkbox"/>
                </composed>
                <composed if="selected">
                    <alias ref="-borderC"/>
                    <alias ref="-icon.checkbox"/>
                </composed>
                <alias ref="-borderE" if="armed"/>
                <alias ref="-borderC"/>
            </select>
        <alias name="checkbox.overlay" ref="-selection"/>

        <alias name="popupmenu.button.background" ref="-gradC" if="hover | armed"/>
        <area name="popupmenu.separator" inset="2" xywh="14,41,1,2"/>
        
        <alias name="buttonBox.background" ref="-borderA" inset="0,-2,-2,-2"/>

        <select name="treetable.treebutton.background" inset="2,1,0,4">
            <area unless="selected" xywh="0,41,9,9"/>
            <area xywh="10,41,9,9"/>
        </select>
        <composed name="columnHeader.background" inset="-3,0">
            <alias ref="-borderB"/>
            <grid weightsX="1,0" weightsY="1">
                <alias ref="none"/>
                <select center="true" inset="0,0,0,3">
                    <alias ref="-iconD" if="sortAscending"/>
                    <alias ref="-iconA" if="sortDescending"/>
                </select>
            </grid>
        </composed>

        <alias name="table.row.background" ref="-gradC" if="rowSelected"/>
        <alias name="table.row.overlay" ref="--selection" inset="1" if="keyboardFocus + !hasFocusedChild + leadRow"/>
        <alias name="table.row.dropmarker" ref="-gradC"/>

        <alias name="propertysheet.sublist.background" ref="-borderB"/>

        <select name="treepathdisplay.node.background">
            <alias ref="-borderE" if="armed"/>
            <alias ref="-borderD" if="hover"/>
        </select>

        <composed name="fileselector.buttonOneLevelUp.background">
            <alias ref="button.background"/>
            <select center="true">
                <alias ref="-iconH" unless="armed"/>
                <alias ref="-iconH" inset="1,1,-1,-1"/>
            </select>
        </composed>
        <composed name="fileselector.buttonHome.background">
            <alias ref="button.background"/>
            <select center="true">
                <alias ref="-iconR" unless="armed"/>
                <alias ref="-iconR" inset="1,1,-1,-1"/>
            </select>
        </composed>
        <select name="-fileselector.showbutton">
            <alias ref="-borderE" if="armed"/>
            <alias ref="-borderF" if="selected"/>
            <alias ref="-borderD" if="hover"/>
        </select>
        <composed name="fileselector.buttonShowFolders.background">
            <alias ref="-fileselector.showbutton"/>
            <alias ref="-iconI" center="true"/>
        </composed>
        <composed name="fileselector.buttonShowHidden.background">
            <alias ref="-fileselector.showbutton"/>
            <alias ref="-iconJ" center="true"/>
        </composed>
        <composed name="fileselector.buttonRefresh.background">
            <alias ref="-fileselector.showbutton"/>
            <alias ref="-iconK" center="true"/>
        </composed>
        <composed name="fileselector.buttonMRU.background">
            <alias ref="button.background"/>
            <select center="true">
                <alias ref="-iconL" unless="armed"/>
                <alias ref="-iconL" inset="1,1,-1,-1"/>
            </select>
        </composed>

        <alias name="colorselector.cursor" ref="-selH"/>
    </images>

    <include filename="cursors.xml"/>
    
    <fontDef name="normal" filename="font.fnt" color="black">
        <fontParam if="error" color="red"/>
        <fontParam if="disabled" color="gray"/>
        <fontParam if="textSelection" color="white"/>
    </fontDef>
    <fontDef name="combobox" filename="font.fnt" color="black">
        <fontParam if="comboboxKeyboardFocus" color="white"/>
    </fontDef>

    <inputMapDef name="-defaultInputMap">
        <action name="selectAll">ctrl A</action>
        <action name="cut">ctrl X</action>
        <action name="copy">ctrl C</action>
        <action name="paste">ctrl V</action>
    </inputMapDef>
    
    <theme name="-defaults">
        <param name="background"><image>none</image></param>
        <param name="overlay"><image>none</image></param>
        <param name="font"><font>normal</font></param>
        <param name="textAlignment"><enum type="alignment">left</enum></param>
        <param name="minWidth"><int>0</int></param>
        <param name="minHeight"><int>0</int></param>
        <param name="maxWidth"><int>0</int></param>
        <param name="maxHeight"><int>0</int></param>
        <param name="inputMap"><inputMap>-defaultInputMap</inputMap></param>
        <!--
            a wildcard import in the base theme allows to use
            any top level theme which has allowWildcard="true"
            as child theme in any other theme (unless overriden).
        -->
        <theme name="" ref="*"/>
    </theme>

    <!-- the fallback theme is used when the requested theme can't be found -->
    <theme name="*" ref="-defaults"/>
    
    <theme name="button" ref="-defaults" allowWildcard="true">
        <param name=""><image>button.*</image></param>
        <param name="border"><border>background.border</border></param>
        <param name="textAlignment"><enum type="alignment">center</enum></param>
    </theme>

    <theme name="togglebutton" ref="button" allowWildcard="true">
        <param name=""><image>togglebutton.*</image></param>
        <param name="border"><border>background.border</border></param>
    </theme>

    <theme name="radiobutton" ref="button" allowWildcard="true">
        <param name=""><image>radiobutton.*</image></param>
        <param name="border"><border>background.border</border></param>
    </theme>

    <theme name="checkbox" ref="-defaults" allowWildcard="true">
        <param name=""><image>checkbox.*</image></param>
        <param name="minWidth"><int>13</int></param>
        <param name="minHeight"><int>13</int></param>
    </theme>

    <theme name="label" ref="-defaults" allowWildcard="true">
    </theme>

    <theme name="editfield" ref="label" allowWildcard="true">
        <param name=""><image>editfield.*</image></param>
        <param name="border"><border>background.border</border></param>
        <param name="passwordChar"><int>0x25CF</int></param>
        <param name="maxWidth"><int>32767</int></param>
        <param name="columns"><int>5</int></param>
        <theme name="renderer" ref="label"/>
        <param name="mouseCursor"><cursor>cursor.text</cursor></param>
        <param name="autocompletion-height"><int>150</int></param>
    </theme>
    <theme name="editfield-errorinfowindow" ref="-defaults">
        <param name="background"><image>-borderG</image></param>
        <param name="border"><border>background.border</border></param>
        <theme name="label" ref="label"/>
    </theme>

    <theme name="tooltipwindow" ref="-defaults">
        <param name="background"><image>-borderG</image></param>
        <param name="border"><border>background.border</border></param>
        <param name="fadeInTime"><int>0</int></param>
        <theme name="label" ref="label"/>
    </theme>

    <theme name="hscrollbar" ref="-defaults" allowWildcard="true">
        <param name="background"><image>scrollbar.background</image></param>
        <theme name="leftbutton" ref="-defaults">
            <param name=""><image>hscrollbar.leftbutton.*</image></param>
            <param name="minWidth"><int>16</int></param>
        </theme>
        <theme name="rightbutton" ref="-defaults">
            <param name=""><image>hscrollbar.rightbutton.*</image></param>
            <param name="minWidth"><int>16</int></param>
        </theme>
        <theme name="thumb" ref="-defaults">
            <param name=""><image>scrollbar.thumb.*</image></param>
        </theme>
        <param name="scaleThumb"><bool>true</bool></param>
        <param name="minWidth"><int>48</int></param>
        <param name="minHeight"><int>16</int></param>
    </theme>

    <theme name="vscrollbar" ref="-defaults" allowWildcard="true">
        <param name="background"><image>scrollbar.background</image></param>
        <theme name="upbutton" ref="-defaults">
            <param name=""><image>vscrollbar.upbutton.*</image></param>
            <param name="minHeight"><int>16</int></param>
        </theme>
        <theme name="downbutton" ref="-defaults">
            <param name=""><image>vscrollbar.downbutton.*</image></param>
            <param name="minHeight"><int>16</int></param>
        </theme>
        <theme name="thumb" ref="-defaults">
            <param name=""><image>scrollbar.thumb.*</image></param>
        </theme>
        <param name="scaleThumb"><bool>true</bool></param>
        <param name="minWidth"><int>16</int></param>
        <param name="minHeight"><int>48</int></param>
    </theme>

    <fontDef name="listbox-display" filename="font.fnt" color="black">
        <fontParam if="selected" color="white"/>
    </fontDef>
    <theme name="listbox" ref="-defaults" allowWildcard="true">
        <param name="background"><image>-borderC</image></param>
        <param name="border"><border>background.border</border></param>
        <param name="cellHeight"><int>font.lineHeight + 4</int></param>
        <param name="cellWidth"><constant>SINGLE_COLUMN</constant></param>
        <param name="rowMajor"><bool>true</bool></param>
        <param name="fixedCellWidth"><bool>false</bool></param>
        <param name="fixedCellHeight"><bool>false</bool></param>
        <param name="maxHeight"><int>8000</int></param>
        <param name="maxWidth"><int>8000</int></param>
        <param name="minDisplayedRows"><int>1</int></param>
        <theme name="vscrollbar" ref="vscrollbar"/>
        <theme name="display" ref="label">
            <param name=""><image>listbox.display.*</image></param>
            <param name="font"><font>listbox-display</font></param>
            <param name="border"><border>2</border></param>
        </theme>
    </theme>

    <theme name="combobox" ref="-defaults" allowWildcard="true">
        <param name="background"><image>-borderC</image></param>
        <theme name="display" ref="label">
            <param name=""><image>combobox.display.*</image></param>
            <param name="border"><border>3</border></param>
            <param name="minWidth"><int>50</int></param>
            <param name="textAlignment"><enum type="alignment">left</enum></param>
            <param name="font"><font>combobox</font></param>
        </theme>
        <theme name="button" ref="-defaults">
            <param name=""><image>combobox.button.*</image></param>
            <param name="minWidth"><int>18</int></param>
        </theme>
    </theme>

    <theme name="comboboxPopup" ref="-defaults">
        <param name="background"><image>-borderH</image></param>
        <param name="border"><border>2</border></param>
        <theme name="listbox" ref="listbox">
            <param name="background"><image>none</image></param>
            <param name="border"><border>0</border></param>
            <param name="minDisplayedRows"><int>5</int></param>
        </theme>
        <param name="maxHeight"><int>150</int></param>
    </theme>

    <theme name="scrollpane" ref="-defaults" allowWildcard="true">
        <theme name="hscrollbar" ref="hscrollbar"/>
        <theme name="vscrollbar" ref="vscrollbar"/>
        <param name="minWidth"><int>100</int></param>
        <param name="minHeight"><int>100</int></param>
        <param name="maxWidth"><int>32767</int></param>
        <param name="maxHeight"><int>32767</int></param>
        <param name="hasDragButton"><bool>false</bool></param>
        <param name="autoScrollArea"><int>5</int></param>
        <param name="autoScrollSpeed"><int>10</int></param>
        <param name="scrollbarsAlwaysVisible"><bool>false</bool></param>
    </theme>

    <theme name="splitpane" ref="-defaults" allowWildcard="true">
        <theme name="splitterHorizontal" ref="button">
            <param name=""><image>splitpane.splitter.*</image></param>
            <param name="border"><border>background.border</border></param>
            <param name="mouseCursor"><cursor>cursor.resizecolumn</cursor></param>
        </theme>
        <theme name="splitterVertical" ref="button">
            <param name=""><image>splitpane.vsplitter.*</image></param>
            <param name="border"><border>background.border</border></param>
            <param name="mouseCursor"><cursor>cursor.resizerow</cursor></param>
        </theme>
    </theme>

    <theme name="dialoglayout" ref="-defaults" allowWildcard="true">
        <param name="defaultGap"><dimension>5,5</dimension></param>
        <param name="smallGap"><dimension>8,8</dimension></param>
        <param name="mediumGap"><dimension>10,10</dimension></param>
        <param name="largeGap"><dimension>15,15</dimension></param>
        <param name="namedGaps"><map/></param>
    </theme>

    <theme name="textarea" ref="-defaults" allowWildcard="true">
        <param name="background"><image>-borderC</image></param>
        <param name="border"><border>background.border</border></param>
        <param name="fonts"><map>
            <param name="default"><font>normal</font></param>
        </map></param>
        <param name="images"><map>
        </map></param>
        <param name="classes"><map>
        </map></param>
    </theme>

    <theme name="-treecellrenderer" ref="-defaults">
        <param name="treeIndent"><int>15</int></param>
        <param name="treeButtonSize"><dimension>14,11</dimension></param>
    </theme>
    <theme name="table" ref="-defaults" allowWildcard="true">
        <param name="rowHeight"><int>font.lineHeight + 6</int></param>
        <theme name="stringcellrenderer" ref="-defaults">
            <param name="font"><font>listbox-display</font></param>
            <param name="border"><border>0,3,0,0</border></param>
        </theme>
        <theme name="TreeLeafCellRenderer" ref="-treecellrenderer"/>
        <theme name="TreeNodeCellRenderer" ref="-treecellrenderer"/>
        <theme name="PropertyEditorCellRender" ref="-treecellrenderer"/>
        <theme name="PropertyListCellRenderer" ref="-treecellrenderer">
            <param name=""><image>propertysheet.sublist.*</image></param>
            <theme name="label" ref="label"/>
        </theme>
